{
  "manifest_version": 2,
  "name": "Search Engines Helper",
  "version": "3.4.0",
  "description": "Add / export and import custom search engines",
  "icons": {
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_title": "Search Engines Helper",
    "browser_style": true,
    "default_popup": "popup.html"
  },
  "web_accessible_resources": ["search-template.xml"],
  "permissions": ["contextMenus", "activeTab", "notifications"],
  "background": {
    "scripts": ["background.js"]
  },
  "applications": {
    "gecko": {
      "strict_min_version": "57.0"
    }
  }
}
